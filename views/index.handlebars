<div class="row">
  <div class="waitarea col s6">
    <h4>Burgers to be Devoured</h4>
    <ul>
      {{#each burgers}}
      {{#unless devoured}}
      {{> burger-block devoured=false}}
      {{/unless}}
      {{/each}}
    </ul>
  </div>
  <div class="trashcan col s6">
    <h4>Burgers Devoured</h4>
    <ul>
      {{#each burgers}}
      {{#if devoured}}
      {{> burger-block devoured=true}}
      {{/if}}
      {{/each}}
    </ul>
  </div>
</div>